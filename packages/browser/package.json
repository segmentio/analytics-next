{
  "name": "@segment/analytics-next",
  "version": "1.38.0",
  "repository": "segmentio/analytics-next",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/pkg/index.js",
  "types": "./dist/pkg/index.d.ts",
  "browser": {
    "./dist/cjs/node": "./dist/cjs/node.browser.js",
    "./dist/cjs/node.js": "./dist/cjs/node.browser.js",
    "./dist/pkg/node": "./dist/pkg/node.browser.js",
    "./dist/pkg/node.js": "./dist/pkg/node.browser.js"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "sideEffects": false,
  "scripts": {
    "build-prep": "sh scripts/build-prep.sh",
    "version": "npm run build-prep && git add src/generated/version.ts",
    "umd": "webpack",
    "build:dev": "yarn clean && yarn build-prep && yarn concurrently 'yarn umd' 'yarn pkg' 'yarn cjs'",
    "build:prod": "NODE_ENV=production yarn clean && yarn build-prep && yarn concurrently 'NODE_ENV=production yarn umd' 'NODE_ENV=production yarn pkg' 'NODE_ENV=production yarn cjs'",
    "pkg": "tsc -p tsconfig.build.json",
    "cjs": "tsc -p tsconfig.build.json --outDir ./dist/cjs --module commonjs",
    "clean": "rm -rf dist",
    "lint": "yarn constraints && tsc --noEmit && eslint '**/*.{js,jsx,ts,tsx}'",
    "test": "jest"
  },
  "size-limit": [
    {
      "path": "dist/umd/index.js",
      "limit": "25.83 KB"
    }
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn workspace @segment/analytics-next eslint --fix"
    ],
    "*.{yml,md,json}": [
      "yarn workspace @segment/analytics-next prettier --write"
    ]
  },
  "resolutions": {
    "glob-parent": "^5.1.2",
    "dot-prop": "^4.2.1"
  },
  "dependencies": {
    "@lukeed/uuid": "^2.0.0",
    "@segment/analytics.js-video-plugins": "^0.2.1",
    "@segment/facade": "^3.4.9",
    "@segment/tsub": "^0.1.12",
    "dset": "^3.1.2",
    "js-cookie": "3.0.1",
    "node-fetch": "^2.6.7",
    "spark-md5": "^3.0.1",
    "tslib": "^2.4.0",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@size-limit/preset-small-lib": "^5.0.2",
    "@tsconfig/node12": "^1.0.9",
    "@types/flat": "^5.0.1",
    "@types/fs-extra": "^9.0.2",
    "@types/jest": "^28.1.1",
    "@types/jest-dev-server": "^5.0.0",
    "@types/jquery": "^3.5.4",
    "@types/js-cookie": "3.0.1",
    "@types/jsdom": "^16.2.14",
    "@types/lodash": "^4.14.161",
    "@types/md5": "^2.2.0",
    "@types/mime": "^2.0.3",
    "@types/node": "^12.12.14",
    "@types/node-fetch": "^2.5.7",
    "@types/node-statsd": "^0.1.2",
    "@types/serve-handler": "^6.1.0",
    "@types/sinon": "^9.0.10",
    "@types/spark-md5": "^3.0.2",
    "@types/test-listen": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@wdio/cli": "^6.10.5",
    "@wdio/jasmine-framework": "^6.10.4",
    "@wdio/local-runner": "^6.10.5",
    "@wdio/sauce-service": "^7.19.7",
    "@wdio/spec-reporter": "^6.8.1",
    "@wdio/sync": "^6.10.4",
    "add": "^2.0.6",
    "aws-sdk": "^2.814.0",
    "compression-webpack-plugin": "^8.0.1",
    "concurrently": "^7.2.1",
    "delay": "^4.4.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "execa": "^4.1.0",
    "express": "^4.17.1",
    "flat": "^5.0.2",
    "fs-extra": "^9.0.1",
    "jest": "^28.1.0",
    "jest-dev-server": "^6.0.3",
    "jest-environment-jsdom": "^28.1.1",
    "jquery": "^3.5.1",
    "jsdom": "^19.0.0",
    "lighthouse": "^9.5.0",
    "log-update": "^4.0.0",
    "micro-memoize": "^4.0.9",
    "mime": "^2.4.6",
    "ngrok": "^4.3.1",
    "node-gyp": "^9.0.0",
    "np": "^7.6.1",
    "p-map": "^4.0.0",
    "playwright": "^1.20.0",
    "prettier": "^2.6.2",
    "serve": "^12.0.0",
    "serve-handler": "^6.1.3",
    "sinon": "^9.2.4",
    "size-limit": "^5.0.2",
    "terser-webpack-plugin": "^5.1.4",
    "test-listen": "^1.1.0",
    "ts-jest": "^28.0.4",
    "ts-loader": "^9.1.1",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4",
    "wdio-chromedriver-service": "^6.0.4",
    "webpack": "^5.36.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0"
  },
  "packageManager": "yarn@3.2.1"
}
