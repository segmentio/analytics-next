{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "dist/pkg/index.d.ts",
  "docModel": {
    "enabled": false
  },
  "apiReport": {
    "enabled": true,
    "reportFolder": "./api"
  },
  "dtsRollup": {
    "enabled": false
  },
  "messages": {
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    },
    "extractorMessageReporting": {
      "ae-missing-release-tag": {
        "logLevel": "none"
      },
      "ae-unresolved-link": {
        "logLevel": "none"
      }
    }
  }
}
